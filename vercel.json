{
    "version": 2,
    "builds": [
        {
          "src": "** /*.py",
          "use": "@vercel/python"
        }
      ],
      "routes": [
        {
          "src": "/test",
          "dest": "api/test.py"
        },
        {
          "src": "/flask",
          "dest": "api/flask_app.py"
        }
      ]    
    ]
  }